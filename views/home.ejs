<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="">
  <meta name="author" content="Dashboard">
  <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
  <title>Stock Register</title>
  <link type="image/png" href="images/adityalogo.png" rel="icon">
  <!-- Bootstrap core CSS-->
  <link rel="stylesheet" href="stylesheets/bootstrap.min.css">
  <script src="javascripts/jquery-1.11.1.min.js"></script>
  <script src="javascripts/bootstrap.min.js"></script>
  <!-- external css-->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <!-- Custom styles for this template-->
  <link href="stylesheets/style.css" rel="stylesheet">
  <link href="stylesheets/style-responsive.css" rel="stylesheet">
  <!--Date Picker-->
  <link href="stylesheets/jquery-ui.css" rel="stylesheet">
  <script type="text/javascript" src="javascripts/angular.min.js"></script>
  <script type="text/javascript" src="javascripts/stock.js"></script>
  <style>
    .panel-box {
      display: table;
      table-layout: fixed;
      width: 100%;
      height: 30px;
      text-align: center;
      border: medium none;
    }

    .bg-success {
      background-color: #A3C86D !important;
      color: #FFF !important;
    }

    .bg-warning {
      background-color: orange !important;
      color: #FFF !important;
    }

    .bg-light {
      background-color: lightblue !important;
      color: #FFF !important;
    }

    .bg-lightgreen {
      background-color: lightgreen !important;
      color: #FFF !important;
    }

    .bg-default {
      background-color: grey !important;
      color: #FFF !important;
    }

    .bg-pink {
      background-color: pink !important;
      color: #FFF !important;
    }

    .bg-tomato {
      background-color: tomato !important;
      color: #FFF !important;
    }

    .bg-dodgerblue {
      background-color: dodgerblue !important;
      color: #FFF !important;
    }

    .bg-violet {
      background-color: violet !important;
      color: #FFF !important;
    }

    .success-text {
      color: #82b33a;
    }

    .bg-danger {
      background-color: #FF7857 !important;
      color: #FFF !important;
    }

    .danger-text {
      color: #d15b3d;
    }

    .panel-box .panel-icon {
      display: table-cell;
      padding: px;
      width: 1%;
      vertical-align: top;
      border-radius: 0px;
      text-align: center;
      /*border-bottom: 1px solid #fcfcfc;
      border-top: 1px solid #fcfcfc;
      border-right: 1px solid #fcfcfc;*/
    }

    .panel-box .size-h1 {
      margin-top: 7px;
    }

    .panel-box .fa {
      margin-top: 9px;
    }

    .bg-lovender {
      background-color: #8075C4 !important;
      color: #FFF !important;
    }

    .lovender-text {
      color: #6a5fb1;
    }

    .capitalize {
      text-transform: capitalize;
    }

    .uppercase {
      text-transform: uppercase;
    }
  </style>
</head>
<body ng-app="stockapp">
  <section id="container">
    <!-- header start-->
    <header class="header black-bg">
      <div class="sidebar-toggle-box">
        <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
      </div>
      <logo start><a class="logo" href="#"><b><img src="images/logo.png" style="height:55px;width:100px;margin-top:-13px"></b></a>
      <logo end>
      <div class="top-menu">
        <ul class="nav pull-right top-menu">
          <li><a class="logout" href="/logout">Logout</a></li>
        </ul>
      </div>
    </header>
    <!-- header end-->
    <!-- sidebar start-->
    <aside>
      <div class="nav-collapse" id="sidebar">
        <!-- sidebar menu start-->
        <ul class="sidebar-menu" id="nav-accordion">
          <p class="centered"><a href="#"><i class="fa fa-user-circle" style="font-size:50px"></i></a></p>
          <h5 class="centered">
            <p>Stock Register</p>
            <p><%= user.username %> </p>
          </h5>
          <li class="active"><a data-toggle="tab" href="#stock"><i class="fa fa-address-card"></i><span>New Stock</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#list"><i class="fa fa-bars"></i><span>List</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#recieve"><i class="fa fa-bars"></i><span>Recieve Stock</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#transfer"><i class="fa fa-bars"></i><span>Transfered Stock</span></a></li>
          <li class="sub-menu"><a data-toggle="tab" href="#condemend"><i class="fa fa-bars"></i><span>Condemend Stock</span></a></li>
        </ul>
        <!-- sidebar menu end-->
      </div>
    </aside>
    <!-- sidebar end-->
    <!-- main content start-->
    <section id="main-content">
      <section class="wrapper site-min-height">
        <div class="row mt">
          <div class="col-lg-12">
            <div class="tab-content">
              <div class="tab-pane fade in active" id="stock" ng-controller="stockcontroller"> <h2> New stock </h2>
                <form class="form-group" style="margin-top: 20px;padding:15px">
                  <label >college</label>
                  <input type="text" ng-model="stock.college" placeholder="Enter college name " class="form-control">
                  <label>department</label>
                  <input type="text" ng-model="stock.Department" class="form-control" placeholder="Enter Department">
                  <label>lab name </label>
                  <input type="text" ng-model="stock.name" class="form-control" placeholder="Enter lab name">
                  <label>date of receipe</label>
                  <input type="date" ng-model="stock.date" class="form-control" placeholder="Enter date of receipe">
                  <label>supplier name</label>
                  <input type="text" ng-model="stock.supplier" class="form-control " placeholder="Enter supplier name">
                  <label>supplier address</label>
                  <input type="text" ng-model="stock.address" class="form-control " placeholder="Enter supplier address">
                  <label>invioce number</label>
                  <input type="number" ng-model="stock.invioce" class="form-control " placeholder="Enter invioce number ">
                  <label>cost per unit</label>
                  <input type="number" ng-model="stock.cost" class="form-control" placeholder="Enter cost per unit">
                  <label>quantity</label>
                  <input type="text" ng-model="stock.quantity" class="form-control" placeholder="Enter quantity">
                  <label>total cost</label>
                  <input type="text" ng-model="stock.total" class="form-control" placeholder="Enter total cost">
                  <label>remarks</label>
                  <input type="text" ng-model="stock.remarks" class="form-control" placeholder="Enter remarks">
                  <label>specifications</label>
                  <br>
                  <textarea class="form-control" ng-model="stock.specifications"> </textarea>
                  <br>
                  <button class="btn btn-info" ng-click="newstock(stock)">submit</button>
                </form>
              </div>
              <div class="tab-pane fade align" id="list" ng-controller="listController" ng-init="getData()"> <h2> List </h2>
                <div class="container" style="background-color: #eaeaea;width: 100%;height:100%;border-radius: 1px">
                  <div class="row"> <br />
                    <div class="col-lg-12">
                      <div class="panel panel-default">
                        <div class="panel-body">
                          <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover">
                              <thead>
                                <tr>
                                  <th>college</th>
                                  <th>departmant</th>
                                  <th>lab name</th>
                                  <th>date of receipe</th>
                                  <th>supplier name</th>
                                  <th>supplier address</th>
                                  <th>invioce number</th>
                                  <th>cost per unit</th>
                                  <th>quantity</th>
                                  <th>total cost</th>
                                  <th>remarks</th>
                                  <th>specifications</th>
                                  <th></th>
                                  <th>Action</th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr ng-repeat="stock in stock1">
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.college}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.college">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.Department}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.Department">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.name}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.name">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.date}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.date">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.supplier}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.supplier">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.address}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.address">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.invioce}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.invioce">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.cost}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.cost">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.quantity}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.quantity">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.total}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.total">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.remarks}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.remarks">
                                  </td>
                                  <td>
                                    <span ng-hide="stockEdit">{{stock.specifications}}</span>
                                    <input ng-show="stockEdit" type="text" ng-model="stock.specifications">
                                  </td>
                                  <td>
                                    <select ng-show="stockTransfer" type="text" ng-model="stock.username">
                                      <option>srinu</option>
                                      <option>Admin</option>
                                    </select>
                                  </td>
                                  <td style="white-space:nowrap;">
                                    <i class="btn btn-warning fa fa-edit" ng-hide="stockEdit" ng-click="stockEdit=true"></i>
                                    <i class="btn btn-primary fa fa-save" ng-show="stockEdit" ng-click="stockEdit=false;editStock(stock)"></i>
                                    <i class="btn btn-danger fa fa-trash" ng-click="deletestock(stock)"> </i>
                                    <i class="btn btn-primary fa fa-share" ng-hide="stockTransfer" ng-click="stockTransfer=true"></i>
                                    <i class="btn btn-success fa fa-check" ng-show="stockTransfer" ng-click="stockTransfer=false;transferData(stock)"></i>
                                    <i class="btn btn-info fa fa-remove" ng-click="condemendStock(stock)"></i>
                                  </td>
                                </tr>
                              </tbody>
                            </table>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
             <div class="tab-pane fade align" id="recieve" ng-controller="recievedController" ng-init="getrecievedData">
              <div class="container" style="background-color: #eaeaea;width: 100%;height:100%;border-radius: 1px">
                  <div class="row"> <br />
                    <div class="col-lg-12">
                      <div class="panel panel-default">
                        <div class="panel-body">
                          <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover"> <h2> Recieve stock </h2>
                              <thead>
                                <tr>
                                  <th>college</th>
                                  <th>departmant</th>
                                  <th>lab name</th>
                                  <th>date of receipe</th>
                                  <th>supplier name</th>
                                  <th>supplier address</th>
                                  <th>invioce number</th>
                                  <th>cost per unit</th>
                                  <th>quantity</th>
                                  <th>total cost</th>
                                  <th>remarks</th>
                                  <th>specifications</th>
                                  <th>Action</th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr ng-repeat="stock in recievedStock">
                                  <td>
                                    <span>{{stock.college}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.Department}}</span>
                
                                  </td>
                                  <td>
                                    <span>{{stock.name}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.date}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.supplier}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.address}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.invioce}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.cost}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.quantity}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.total}}</span>
            
                                  </td>
                                  <td>
                                    <span>{{stock.remarks}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.specifications}}</span>
                    
                                  </td>
                                  <td style="white-space:nowrap;">
                                    <i class="btn btn-primary" ng-click="getrecievedData">Recieve</i>
                                  </td>
                                </tr>
                              </tbody>
                            </table>
                            </div>
                             </div>
                              </div>
                               </div>
                                </div>
                                 </div>

              </div>
              <div class="tab-pane fade align" id="transfer" ng-controller="transferController">
               <div class="container" style="background-color: #eaeaea;width: 100%;height:100%;border-radius: 1px">
                  <div class="row"> <br />
                    <div class="col-lg-12">
                      <div class="panel panel-default">
                        <div class="panel-body">
                          <div class="table-responsive">
                <table class="table table-striped table-bordered table-hover"> <h2> Transfered Stock </h2>
                              <thead>
                                <tr>
                                  <th>college</th>
                                  <th>departmant</th>
                                  <th>lab name</th>
                                  <th>date of receipe</th>
                                  <th>supplier name</th>
                                  <th>supplier address</th>
                                  <th>invioce number</th>
                                  <th>cost per unit</th>
                                  <th>quantity</th>
                                  <th>total cost</th>
                                  <th>remarks</th>
                                  <th>specifications</th>
                                  <th>Action</th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr ng-repeat="stock in stock1">
                                  <td>
                                    <span>{{stock.college}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.Department}}</span>
                
                                  </td>
                                  <td>
                                    <span>{{stock.name}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.date}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.supplier}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.address}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.invioce}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.cost}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.quantity}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.total}}</span>
            
                                  </td>
                                  <td>
                                    <span>{{stock.remarks}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.specifications}}</span>
                                </tr>
                              </tbody>
                            </table>
                            </div>
                            </div>
                            </div>
                            </div>
                            </div>
                            </div>

              </div>
              <div class="tab-pane fade align" id="condemend" ng-controller="condemendController" ng-init="getcondemendData()">
                <div class="container" style="background-color: #eaeaea;width: 100%;height:100%;border-radius: 1px">
                  <div class="row"> <br />
                    <div class="col-lg-12">
                      <div class="panel panel-default">
                        <div class="panel-body">
                          <div class="table-responsive">
                            <table class="table table-striped table-bordered table-hover"> <h2> Condemend Stock </h2>
                              <thead>
                                <tr>
                                  <th>college</th>
                                  <th>departmant</th>
                                  <th>lab name</th>
                                  <th>date of receipe</th>
                                  <th>supplier name</th>
                                  <th>supplier address</th>
                                  <th>invioce number</th>
                                  <th>cost per unit</th>
                                  <th>quantity</th>
                                  <th>total cost</th>
                                  <th>remarks</th>
                                  <th>specifications</th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr ng-repeat="stock in condemendStock">
                                  <td>
                                    <span>{{stock.college}}</span>
                                  
                                  </td>
                                  <td>
                                    <span>{{stock.Department}}</span>
                
                                  </td>
                                  <td>
                                    <span>{{stock.name}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.date}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.supplier}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.address}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.invioce}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.cost}}</span>
          
                                  </td>
                                  <td>
                                    <span>{{stock.quantity}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.total}}</span>
            
                                  </td>
                                  <td>
                                    <span>{{stock.remarks}}</span>
              
                                  </td>
                                  <td>
                                    <span>{{stock.specifications}}</span>
                    
                                  </td>
                                </tr>
                              </tbody>
                            </table>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
          </div>
        </div>
      </div>
    </section>
  </section>
</section>
<script src="javascripts/jquery.js"></script>
